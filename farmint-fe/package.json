{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "node scripts/clean-build.js",
    "check-env": "node scripts/check-env.js",
    "check-ui-deps": "node scripts/check-ui-deps.js",
    "find-unused": "node scripts/find-unused-files.js",
    "remove-unused": "node scripts/remove-unused-files.js",
    "performance": "node scripts/performance-check.js",
    "prebuild": "npm run clean",
    "build": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "build:safe": "npm run clean && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "build:debug": "npm run check-env && npm run clean && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "build:netlify": "npm install --legacy-peer-deps && npm run build",
    "build:analyze": "npm run build && npm run performance",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@farcaster/auth-kit": "^0.2.0",
    "@farcaster/core": "^0.14.0",
    "@farcaster/frame-sdk": "^0.0.57",
    "@hookform/resolvers": "^3.9.0",
    "@next/swc-wasm-nodejs": "13.5.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tabs": "^1.1.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.446.0",
    "next": "13.5.1",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "cross-env": "^7.0.3",
    "encoding": "^0.1.13",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "postcss": "8.4.30"
  }
}
